<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent"
    tools:context=".view.activity.teamreport.KuisActivity">
    <include layout="@layout/action_bar" android:id="@+id/bar"/>

    <LinearLayout
        android:id="@+id/llBulan1"
        android:layout_below="@+id/bar"
        android:background="@color/black"
        android:layout_height="wrap_content"
        android:layout_width="match_parent">
        <TextView
            android:id="@+id/tvBulan1"
            android:layout_margin="@dimen/dp8"
            android:textSize="@dimen/dp20"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:text="Bulan Ke 1"
            android:textColor="@color/white"
            tools:ignore="Suspicious0dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/llPKBulan1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/llBulan1"
        android:background="@color/abu"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:padding="@dimen/dp8">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/tv_titlePK1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="@dimen/dp20"
                    tools:ignore="Suspicious0dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tvPKTanggalBulan1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="@dimen/dp12"
                    tools:ignore="Suspicious0dp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_gravity="right"
            android:layout_weight="1"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="@dimen/dp80"
                android:layout_height="@dimen/dp30"
                android:src="@drawable/cv_right" />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/llNOSBulan1"
        android:visibility="gone"
        android:padding="@dimen/dp8"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:layout_below="@+id/llPKBulan1"
        android:background="@color/white"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_weight="5"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_width="0dp">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/tv_titleNOS1"
                    android:textSize="@dimen/dp20"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:text=""
                    android:textColor="@color/black"
                    tools:ignore="Suspicious0dp" />
