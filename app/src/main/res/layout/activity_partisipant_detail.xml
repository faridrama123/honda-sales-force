                android:gravity="center"
                android:text="0"
                android:textColor="@color/white"
                tools:ignore="Suspicious0dp"
                app:drawableRightCompat="@drawable/cv_right"  />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/linear_wsp_noikt"

        android:layout_margin="@dimen/dp8"
        android:orientation="horizontal"
        android:layout_below="@+id/linear_wsp_ikt"
        android:background="@color/white"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <LinearLayout

            android:layout_weight="1"
            android:layout_height="fill_parent"
            android:layout_width="0dp">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:text="Tidak Mengikuti Kuis"
                android:textColor="@color/black"
                android:textSize="@dimen/dp16"
                tools:ignore="Suspicious0dp" />
        </LinearLayout>
        <LinearLayout

            android:id="@+id/wsp_noikt_click"

            android:background="@color/reds"
            android:layout_weight="1"
            android:layout_height="fill_parent"
            android:layout_width="0dp">
            <TextView
