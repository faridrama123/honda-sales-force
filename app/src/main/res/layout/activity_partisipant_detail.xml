<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent"
   >

    <include layout="@layout/action_bar" android:id="@+id/bar"/>

    <LinearLayout
        android:id="@+id/llTanggal"
        android:layout_margin="@dimen/dp8"
        android:orientation="horizontal"
        android:layout_below="@+id/bar"
        android:background="@color/white"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <LinearLayout

            android:layout_weight="1"
            android:layout_height="fill_parent"
            android:layout_width="0dp">
            <TextView
                android:id="@+id/startdate"
                android:textSize="@dimen/dp14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="00-December-0000"
                android:textColor="@color/black"
                tools:ignore="Suspicious0dp" />
        </LinearLayout>
        <LinearLayout

            android:layout_weight="0.2"
            android:layout_height="fill_parent"
            android:layout_width="0dp">
            <TextView
                android:textSize="@dimen/dp14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="-"
                android:textColor="@color/black"
                tools:ignore="Suspicious0dp" />
        </LinearLayout>
        <LinearLayout
            android:layout_weight="1"
            android:layout_height="fill_parent"
            android:layout_width="0dp">
            <TextView
                android:id="@+id/enddate"
                android:textSize="@dimen/dp14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:text="00-December-0000"
                android:textColor="@color/black"
                tools:ignore="Suspicious0dp" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ctitle_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/llTanggal"
        android:background="@color/black">

        <TextView
            android:id="@+id/title_1"

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/dp8"
            android:gravity="left"
            android:text="title 1"
            android:textColor="@color/white"
            android:textSize="@dimen/dp20"
            tools:ignore="Suspicious0dp" />

    </LinearLayout>
    <LinearLayout
        android:id="@+id/csubtitle_1"
        android:layout_below="@+id/ctitle_1"
        android:background="@color/black"
        android:layout_height="wrap_content"
        android:layout_width="match_parent">
        <TextView
            android:layout_margin="@dimen/dp8"
            android:layout_marginTop="@dimen/dp3"
            android:layout_marginLeft="@dimen/dp8"
            android:textSize="@dimen/dp20"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/subtitle_1"

            android:gravity="left"
            android:text="subtitle 1"
            android:textColor="@color/white"
            tools:ignore="Suspicious0dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearRegularIkt"
        android:layout_margin="@dimen/dp8"
        android:orientation="horizontal"
        android:layout_below="@+id/csubtitle_1"
        android:background="@color/white"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_weight="1"
            android:layout_height="fill_parent"
            android:layout_width="0dp">
            <TextView
                android:textSize="@dimen/dp16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:text="Mengikuti Kuis"
                android:textColor="@color/black"
                tools:ignore="Suspicious0dp" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/regularIktClick"
            android:background="@color/colorPrimary"
            android:layout_weight="1"
            android:layout_height="fill_parent"
            android:layout_width="0dp">
            <TextView
                android:id="@+id/regularIktCount"
                android:textSize="@dimen/dp20"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="0"
                android:textColor="@color/white"
                tools:ignore="Suspicious0dp"
                app:drawableRightCompat="@drawable/cv_right" />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/linearRegularNoIkt"
        android:layout_margin="@dimen/dp8"
        android:orientation="horizontal"
        android:layout_below="@+id/linearRegularIkt"
        android:background="@color/white"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_weight="1"
            android:layout_height="fill_parent"
            android:layout_width="0dp">
            <TextView
                android:textSize="@dimen/dp16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:text="Tidak Mengikuti Kuis"
                android:textColor="@color/black"
                tools:ignore="Suspicious0dp" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/regularNoIktClick"

            android:background="@color/reds"
            android:layout_weight="1"
            android:layout_height="fill_parent"
            android:layout_width="0dp">
            <TextView
                android:id="@+id/regularNoIktCount"
                android:textSize="@dimen/dp20"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="0"
                android:textColor="@color/white"
                tools:ignore="Suspicious0dp"
                app:drawableRightCompat="@drawable/cv_right" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ctitle_2"
        android:layout_below="@+id/linearRegularNoIkt"
        android:background="@color/black"
        android:orientation="vertical"
        android:layout_height="wrap_content"
        android:layout_width="match_parent">
        <LinearLayout
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:layout_width="match_parent">
            <TextView
                android:layout_marginTop="@dimen/dp3"
                android:layout_marginLeft="@dimen/dp8"
                android:textSize="@dimen/dp20"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:id="@+id/title_2"
                android:text="title 2"
                android:textColor="@color/white"
                tools:ignore="Suspicious0dp" />
        </LinearLayout>


    </LinearLayout>

    <LinearLayout
